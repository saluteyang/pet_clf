  "sentences": [
    {
      "text": {
        "content": "Snow is looking for a permanent home...A place she could finally call &quot;HOME&quot;.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": 0.1
      }
    },
    {
      "text": {
        "content": "She had been known as a stray throughout her life and we hope she won't be know as a stray for the rest of her life.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.5,
        "score": 0.5
      }
    },
    {
      "text": {
        "content": "She is loyal, friendly and make a good companion.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "Those interested to welcome her to be part of your family, please do not hesitate to contact me.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": 0.3
      }
    }

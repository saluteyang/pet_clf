  "sentences": [
    {
      "text": {
        "content": "The owner of this toy poodle will be station offshore (oil &amp; gas industry) in Johar and no one can take care of him anymore.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": -0.2
      }
    },
    {
      "text": {
        "content": "I'm putting Milo on adoption on behalf of the owner @ RM0 adoption fee in hope to find a loving and caring new owner who can take good care of Milo.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "He's not certified, but was regularly vaccinated, groomed, already spayed and de-wormed.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": -0.2
      }
    },
    {
      "text": {
        "content": "Interested adopter can drop by for viewing at Pets 4u petshop located at Puchong, Bandar Puteri.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    }

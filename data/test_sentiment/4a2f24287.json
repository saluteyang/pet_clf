  "sentences": [
    {
      "text": {
        "content": "Kiki is about 2 months old and rescued by Nancy Lee, who requested for my help to post on the Internet.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "According to her, Kiki is very active, alert and adorable.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.8,
        "score": 0.8
      }
    },
    {
      "text": {
        "content": "She loves to be pampered and is very intelligent.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "Deworming and 1st vaccination has already been done.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "The vet has checked her condition and confirms that she is very healthy.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "Her 2 brothers, Bamboo &amp; Sumo, have already adopted.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "To view them, please call NANCY LEE at .",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "Location is in Bangsar.",
        "beginOffset": -1
      },

  "sentences": [
    {
      "text": {
        "content": "I'm currently fostering these puppies which were posted on the Facebook page of Malaysia Independent Animal Rescue (MIAR).They were dumped at a construction site at only 2 weeks old.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.6,
        "score": -0.6
      }
    },
    {
      "text": {
        "content": "They're doing well now despite being orphaned at such a young age.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.5,
        "score": 0.5
      }
    },
    {
      "text": {
        "content": "I'm trying my best to look for adopters.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.5,
        "score": 0.5
      }
    },
    {
      "text": {
        "content": "I already have 5 rescued dogs in my home, so I really cannot take them in. :(",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.4,
        "score": -0.4
      }
    },
    {
      "text": {
        "content": "Please give them a forever home..",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": 0.1
      }
    }

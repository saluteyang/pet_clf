    {
      "text": {
        "content": "We're looking for adopters who are interested in these kittens (see my other pet profiles).",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.4,
        "score": 0.4
      }
    },
    {
      "text": {
        "content": "2 have been adopted and there are 5 left.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "The kittens been rescued off the street along with the mother cat and they've all had their first vaccination and deworm.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": 0.2
      }
    },
    {
      "text": {
        "content": "We are humbly collecting an adoption fee of RM 50 each kitten to pay off their medical bills and balance to use it for their mother's neutering surgery.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": -0.2
      }
    },
    {
      "text": {
        "content": "Please Whatsapp for more details.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    }

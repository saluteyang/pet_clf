  "sentences": [
    {
      "text": {
        "content": "Lara &amp; Kayla are two very cute and beautiful teenage cats that are extremely playful and loyal.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.8,
        "score": 0.8
      }
    },
    {
      "text": {
        "content": "They are not the types of cats to run away unless they are scared.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": -0.1
      }
    },
    {
      "text": {
        "content": "They will play with each other as they have been together ever since birth.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": 0.2
      }
    },
    {
      "text": {
        "content": "The reason I need to find them a home is because I can no longer take care of them as I am always out at work and can rarely find time for them as much as I try.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.4,
        "score": -0.4
      }
    },
    {
      "text": {
        "content": "If interested please call or whatsapp me at",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    }

{
  "sentences": [
    {
      "text": {
        "content": "LOUIS IS A OBIDIENT DOG.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": -0.1
      }
    },
    {
      "text": {
        "content": "HE IS VERY FRIENDLY.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "HE HAS A VERY SMOOTH CREAMY FUR AND A BEAUTIFUL EYES.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "WE BOUGHT A PAIR FROM PET SHOP IN OCT 08 WHERE A FEMALE DOG IS CALL CHARMINE AND A MALE IS CALL LOUIS.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": -0.3
      }
    },
    {
      "text": {
        "content": "UNFORTUNATELY WE LOST THE FEMALE AS SHE HAD RAN OUT OF THE HOUSE AND EATEN SOME POISONOUS FOOD AND DIED OF FOOD POISONOUS.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.7,
        "score": -0.7
      }
    },
    {
      "text": {
        "content": "WE MISS HER VERY MUCH.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "SINCE WE LEFT LOUIS ALONE AND WE WILL BE MIGRATE TO OTHER COUNTRY, WE WILL LIKE SOMEONE REALLY COMMITTED IN TAKING OF DOGS TO ADOPT OUR DOG, LOUIS.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": -0.3
      }
    }

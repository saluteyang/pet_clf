  "sentences": [
    {
      "text": {
        "content": "STATUS: NIGHT IS STILL AVAILABLE Night is rescued together with her 2 brothers and sister.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": 0.2
      }
    },
    {
      "text": {
        "content": "This cutie is very playful and quick, ready to turn anything into real fun games.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "She's now ready to be adopted by a real animal lover.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": 0.3
      }
    },
    {
      "text": {
        "content": "Terms and conditions: 1) Adoption is FREE OF CHARGE to real cat lover.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": -0.3
      }
    },
    {
      "text": {
        "content": "2) The kitten must be vaccinated and spayed by the new owner - non-negotiable. 3) No students, please.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.4,
        "score": -0.4
      }
    },
    {
      "text": {
        "content": "New owner must be able to provide for the kittens' needs and she needs a forever home.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.6,
        "score": 0.6
      }
    },
    {
      "text": {
        "content": "Please contact Yanty () or Rudy () for more info.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },

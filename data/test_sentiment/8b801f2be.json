      "text": {
        "content": "For any inquiry or adoption, pls contact: (Call / SMS / E-mail / Viber / Whats-App) Ms Pang May Ki / Mr.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,

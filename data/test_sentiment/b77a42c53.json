  "sentences": [
    {
      "text": {
        "content": "These 3 little kittens were sadly abandoned by their mother after just a few days old.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.5,
        "score": -0.5
      }
    },
    {
      "text": {
        "content": "I brought them back and fortunately, my cat had fostered and lovingly cared for them.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.7,
        "score": 0.7
      }
    },
    {
      "text": {
        "content": "Now these adorable kittens have grown and looking for new adopted parents!",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "Are you one of them?",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "Please contact me if interested.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": -0.2
      }
    },
    {
      "text": {
        "content": "(P/s: Only for serious adopter who will keep them indoor and spay/neuter them)",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.7,
        "score": 0.7
      }
    }

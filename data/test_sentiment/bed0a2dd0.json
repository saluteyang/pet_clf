{
  "sentences": [
    {
      "text": {
        "content": "Anyone want a cat?",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "We found this fellow hanging around downstairs hungry and looking for food.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": 0.3
      }
    },
    {
      "text": {
        "content": "We have been feeding her for the past three weeks.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "We decided to take her in after seeing some kids bullying her.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.6,
        "score": -0.6
      }
    },
    {
      "text": {
        "content": "She is a lovely, well behaved cat, but unfortunately, we cannot keep her as my wife has sinus.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": 0.1
      }
    },
    {
      "text": {
        "content": "We are willing to give her to anyone who is willing to take good care of her",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.6,
        "score": 0.6
      }
    }
  ],

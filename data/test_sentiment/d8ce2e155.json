{
  "sentences": [
    {
      "text": {
        "content": "This Bulldog is friendly but he hates the garbage men, mail men, newspaper men.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": 0.2
      }
    },
    {
      "text": {
        "content": "He also hates brooms and dustpan.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.6,
        "score": -0.6
      }
    },
    {
      "text": {
        "content": "He is not used to wearing a collar or a leash.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "He is suitable to stay alone.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.2,
        "score": 0.2
      }
    },
    {
      "text": {
        "content": "I need to give him away because he cannot get along with my older dog who is a basset hound.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.5,
        "score": -0.5
      }
    },
    {
      "text": {
        "content": "He needs a good home.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.4,
        "score": 0.4
      }
    },
    {
      "text": {
        "content": "No breeders, please.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": -0.3
      }
    }
  ],

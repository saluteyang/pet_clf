  "sentences": [
    {
      "text": {
        "content": "We found Mr. X alone and lethargic in the street, he was dehydrated and undernourished.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.5,
        "score": -0.5
      }
    },
    {
      "text": {
        "content": "We took him to the vet to get him checked out and dewormed.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    },
    {
      "text": {
        "content": "The vet gave him a clean bill of health and after a few days of proper food with us he has gained weight and is now one healthy, energetic, and super cute kitty looking for a loving home.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.9,
        "score": 0.9
      }
    },
    {
      "text": {
        "content": "We would love to keep him but we have 2 other animals and space would be an issue.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.5,
        "score": 0.5
      }
    }

      "text": {
        "content": "Hello,my name Is Alireza.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": 0.1
      }
    },
    {
      "text": {
        "content": "I saved the mother of this cat when she was young.",
        "beginOffset": -1
      },

  "sentences": [
    {
      "text": {
        "content": "Simon and Stanley were both siblings and they were rescued by Second Chance from Jalan Ipoh.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.3,
        "score": 0.3
      }
    },
    {
      "text": {
        "content": "They were only 4 weeks old at the time of rescue and they have been fostered by me and they have grown to be very healthy and ready to go to a new permanent home.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.1,
        "score": 0.1
      }
    },
    {
      "text": {
        "content": "Simon is more active and playful whilst Stanley is more calm and quiet.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.8,
        "score": 0.8
      }
    },
    {
      "text": {
        "content": "Please give them a loving home.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0.7,
        "score": 0.7
      }
    },
    {
      "text": {
        "content": "Please call for Tracy or Mei Leng or Kim for more adoption details.",
        "beginOffset": -1
      },
      "sentiment": {
        "magnitude": 0,
        "score": 0
      }
    }
  ],
